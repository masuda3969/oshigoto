<div class = "container">
  <div class = "row">
    <div class = "col">
        <!--記事一覧表示-->
        <% @articles.each do |article| %>
          <%= link_to article_path(article.id) do %>
            <%= article.title %>
          <% end %>
          <%= article.user.account %>
          <%= image_tag article.user.image %>
        <% end %>
    </div>
  </div>
  <div class = "row">
    <%= link_to "戻る", new_article_path %>
  </div>
</div>








