<header>
 <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
</header>

<%= month_calendar events: schedules do |date, schedules| %>
 <%= date.day %>

  <% schedules.each do |schedule| %>
   <%= link_to schedule_path(schedule.id) do %>
    <%= schedule.title %>
   <% end %>
  <% end %>
<% end %>
