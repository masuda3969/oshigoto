<div class = "container" >
  <div class = "row">
    <%= month_calendar events: @schedules do |date, schedules| %>
     <%= date.day %>

     <% schedules.each do |schedule| %>
     <%= link_to schedule_path(schedule.id) do %>
       <%= schedule.title %>
     <% end %>
     <% end %>
    <% end %>
  </div>
</div>

