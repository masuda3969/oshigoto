
<div class = "container">
  <div class = "row">
    <%= form_with model: @schedule do |f| %>
    <%= f.label :タイトル %>
    <%= f.text_field :title %>
    <%= f.label :スケジュール %>
    <%= f.text_area :body %>
    <%= f.label :日時 %>
    <%= f.datetime_select :start_time %>
    <%= f.submit'変更を保存する' %>
    <% end %>
  </div>
</div>