<div class = "container">
  <div class = "row">
    <% if users.exists? %>
    <table class = "table">
      <thead>
        <tr>
          <th colspan = "1"></th>
          <th>名前</th>
        </tr>
      </thead>
      <tbody>
        <% users.each do |user| %>
        <tr>
          <td><%= image_tag user.image %></td>
          <td><%= link_to user_path(user.id) do %>
            <%= user.account %>
            <% end %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    <% else %>
     <p>ユーザーはいません</p>
    <% end %>
  </div>
  <div class = "row">
    <%= link_to '戻る', :back %>
  </div>
</div>