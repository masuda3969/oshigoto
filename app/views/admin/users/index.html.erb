<div class = "container">
  <div class = "row">
    <h4>会員一覧</h4>
    <table class = "table">
      <thead>
        <tr>
          <th>会員ID</th>
          <th>会員氏名</th>
          <th>ステータス</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td>
            <%= user.id %>
          </td>
          <td>
            <%= link_to admin_user_path(user.id) do %>
             <%= user.last_name %><%= user.first_name %>
            <% end %>
          </td>
          <td>
            <%= user.is_deleted %>
          </td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
